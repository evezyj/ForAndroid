<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent" xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical"

        tools:context="com.one.frontend.interviewexam.ExamlistActivity">
    <com.one.frontend.interviewexam.TopBar
            android:id="@+id/topbar"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#FFFFFF"
            app:leftBackground="@drawable/selector_left"
            app:titleText="O N E"
            app:titleTextColor="#202020"
            app:titleTextSize="15dp" />
    <LinearLayout android:background="#F8F8F8"
                  android:paddingLeft="20dp"
                  android:paddingRight="20dp"
                  android:paddingTop="30dp" android:layout_width="match_parent" android:layout_height="match_parent">
        <ListView android:layout_width="match_parent" android:layout_height="wrap_content"
                  android:id="@+id/list_exam" android:divider="@null"
        >

        </ListView>
    </LinearLayout>

</LinearLayout>
